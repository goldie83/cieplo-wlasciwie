{% extends 'KrakenRankingBundle::base.html.twig' %}

{% block headline %}Salon odrzuconych{% endblock headline %}

{% form_theme form 'KrakenWarmBundle:Form:form_div_layout.html.twig' %}

{% block content_row %}
<div class="row">
    <div class="col-sm-4">
        <form class="form-horizontal" method="POST" action="{{ url('ranking_rejected')}}">
            {{ form_rest(form) }}
            <button type="submit" class="btn btn-primary">Szukaj</button>
        </form>
    </div>
    <div class="col-sm-8">
        {% include('KrakenRankingBundle:Ranking:boilerList.html.twig') with {boilers: boilers, enableSort: false} %}
    </div>
</div>
{% endblock content_row %}
