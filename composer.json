{
    "name": "kraken/warm-calculator",
    "description": "Human-friendly heating balance calculator for apartments and buildings",
    "autoload": {
        "psr-0": { "": "src/", "Context": "features/", "DataFixtures": "features/" }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "1.10.2",
                "dist": {
                    "url": "http://code.jquery.com/jquery-1.10.2.js",
                    "type": "file"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.7.*",
        "doctrine/orm": "~2.2,>=2.2.3,<2.5",
        "doctrine/dbal": "<2.5",
        "doctrine/doctrine-bundle": "~1.4",
        "doctrine/doctrine-fixtures-bundle": "2.2.*",
        "twig/extensions": "~1.0",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~3.0,>=3.0.12",
        "sensio/framework-extra-bundle": "~3.0,>=3.0.2",
        "symfony/assetic-bundle": "2.3.*",
        "sensio/generator-bundle": "2.3.*",
        "jms/security-extra-bundle": "1.5.*",
        "jms/di-extra-bundle": "1.4.*",
        "vich/geographical-bundle": "dev-master",
        "twbs/bootstrap": "3.1.*",
        "jquery/jquery": "1.10.*",
        "knplabs/knp-menu-bundle": "dev-master",
        "doctrine/data-fixtures": "1.0.*@dev",
        "stof/doctrine-extensions-bundle": "1.2.*",
        "white-october/pagerfanta-bundle": "dev-master",
        "friendsofsymfony/jsrouting-bundle": "dev-master",
        "doctrine/migrations": "dev-master",
        "doctrine/doctrine-migrations-bundle": "dev-master",
        "mopa/bootstrap-bundle": "dev-master",
        "raven/raven": "0.12.*",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "vich/uploader-bundle": "~0.14",
        "sonata-project/admin-bundle": "^2.4@dev",
        "sonata-project/doctrine-orm-admin-bundle": "^2.4@dev",
        "sonata-project/formatter-bundle": "^2.4@dev",
        "sonata-project/media-bundle": "dev-master",
        "sonata-project/block-bundle": "dev-master",
        "nelmio/api-doc-bundle": "2.11.*@dev",
        "friendsofsymfony/rest-bundle": "1.7.*@dev",
        "liip/imagine-bundle": "^1.3@dev",
        "beyerz/open-graph-protocol-bundle": "^1.0",
        "sonata-project/classification-bundle": "*"
    },
    "require-dev": {
        "behat/behat": "3.0.*@dev",
        "behat/common-contexts": "*",
        "behat/symfony2-extension": "*",
        "behat/mink": "*",
        "behat/mink-extension": "*",
        "behat/mink-browserkit-driver": "*",
        "mockery/mockery": "dev-master",
        "vipsoft/doctrine-data-fixtures-extension": "*"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "branch-alias": {
            "dev-master": "2.3-dev"
        }
    }
}
